   ],
   "source": [
    "var('x, a')\n",
    "assume(a>0)\n",
    "f(x)=sqrt(x*(1-x))/(x+a)\n",
    "#show(integral(f(x),x))\n",
    "show(integrate(f(x),x,0,1))"
   ]
